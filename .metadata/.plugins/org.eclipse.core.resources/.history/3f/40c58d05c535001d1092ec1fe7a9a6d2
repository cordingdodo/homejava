package homework;

public class for문4 {

	public static void main(String[] args) {
		//0부터 20까지 출력해보자.
		for(int i=0;i<=20;i++){
			System.out.println(i);
		}
		System.out.println();
		System.out.println("******************");
		//0부터 90까지 10단위로 출력해보자.ex. 0, 10, 20, 30, .. , 80, 90
		for(int i=0;i<=90;i+=10){
			System.out.println(i);
			System.out.println();
			System.out.println("******************");
		//0부터 10까지 모두 더한 값을 출력해보자.
			int tot=0;
		for(int a=0;a<=10;a++){
			tot+=a;
			System.out.println(tot);
		}
		}
		

	    

	}

}
